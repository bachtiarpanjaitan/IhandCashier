        <ic-selected>Dark</ic-selected>
        <ic-options>
            <ic-option>Dark</ic-option>
            <ic-option>Light</ic-option>
        </ic-options>
    </ic-thema>
    <ic-data>
        <ic-data_per_halaman>20</ic-data_per_halaman>
